#include<iostream>
int main()
{
	int a,b,c,d;
	a=5;
	b=6;
	c=7;
	d=8;
	if(a>b && a>c)
	{
	 std::cout<<"a is greater";
	}
	else if(b>a && b>c)
	{
	std::cout<<"b is greater";
	}
	else if(c>a && c>d)
	{
	std::cout<<"c is greater";
	}
	else if(d>a && d>c)
	{
	std::cout<<"d is greater";
    }
	return 0;
}
